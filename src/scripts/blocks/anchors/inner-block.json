{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "nf/anchor",
  "version": "0.1.0",
  "title": "Single Anchor Element",
  "category": "neofluxe",
  "icon": "star",
  "description": "A single anchor element",
  "supports": {
    "html": false,
    "align": false,
    "anchor": true
  },
  "textdomain": "neofluxe",
  "attributes": {
    "preTitle": {
      "type": "string",
      "selector": "h6"
    },
    "title": {
      "type": "string",
      "selector": "p"
    },
    "link": {
      "type": "string"
    }
  }
}
